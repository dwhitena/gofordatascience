{
  "pipeline": {
    "name": "filter"
  },
  "transform": {
    "image": "dwhitena/repofilter",
    "cmd": [ "/repofilter" ],
    "stdin": []
  },
  "parallelism": "1",
  "inputs": [
    {
      "repo": {
        "name": "repodata"
      },
      "method": "map"
    }
  ]
}
{
  "pipeline": {
    "name": "compile16"
  },
  "transform": {
    "image": "dwhitena/golangbuilder16",
    "cmd": [ "sh" ],
    "stdin": [
        "for filename in /pfs/filter/*; do",
		"REPONAME=`cat /pfs/filter/$filename`",
		"source /build.sh > /pfs/out/$filename",
	"done"
    ]
  },
  "parallelism": "1",
  "inputs": [
    {
      "repo": {
        "name": "filter"
      },
      "method": "map"
    }
  ]
}
